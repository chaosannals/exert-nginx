ngx_addon_name=ngx_http_exert_module

EXERT_SRCS="
    $ngx_addon_dir/ngx_http_exert_module.c \
"

EXERT_DEPS="
    $ngx_addon_dir/ngx_http_exert_module.h \
"

if [ -n "$ngx_module_link" ]; then
    ngx_module_type=HTTP
    ngx_module_name=$ngx_addon_name
    ngx_module_incs=
    ngx_module_deps=
    ngx_module_srcs="$EXERT_SRCS"
    ngx_module_libs=
    . auto/module
else
    HTTP_MODULES="$HTTP_MODULES ngx_http_exert_module"
    NGX_ADDON_DEPS=
    NGX_ADDON_SRCS="$NGX_ADDON_SRCS $EXERT_SRCS"
fi
